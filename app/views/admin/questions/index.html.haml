%h1
  =@test.title
%table.table.table-striped.table-sm
  %thead
    %th{scope: "col"}
      ID
    %th{scope: "col"}
      Body
    %th{scope: "col", colspan: 3}
      Actions
  %tbody
    = render @test.questions
%p
  = link_to t(".new_question"), new_admin_test_question_path(@test), class: "btn btn-primary"
%p
  = link_to "Edit Test", edit_admin_test_path(@test), class: "btn btn-primary"
%p
  = link_to t(".back"), admin_tests_path, class: "btn btn-primary"
