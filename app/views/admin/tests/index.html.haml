%h1 Tests List
= link_to t(".new_test"), new_admin_test_path
%table
  %thead
    %tr
      - human_names(Test, [:id, :title, :level, :category, :author]).each do |name|
        %th
          = name
      %th
        = t(".questions_count")
      %th{colspan: 2}
        = t(".actions")
  %tbody
    = render @tests
