%h1
  Test:
  = @test_passage.test.title
%p
  = t(".your_result")
  - if @test_passage.success?
    %span.success_test
      = t(".success")
      = @test_passage.percent
      \%
  -else
    %span.failed_test
      = t(".failed")
      = @test_passage.percent
      \%

%p
  = link_to t(".return"), tests_path
